<div>
	<input type="text" placeholder="@InputPlaceholder" @bind="InputValue"/>
	<ul>
		@foreach (string result in SearchResults.Where(x=>x.Contains(InputValue)))
		{
			<li>result</li>
		}
	</ul>
</div>

@code {
	[Parameter]
	public string InputPlaceholder { get; set; } = "Search...";
	[Parameter]
	public string[] SearchResults { get; set; }
	private string InputValue;



}
